{
  "name": "name-fantasy",
  "resultPattern": "{result}",
  "globals": {
    "culture": "generic fantasy"
  },
  "variables": {
    "footnote": ""
  },
  "explanation": "Generic fantasy names not based on a real-world culture.",
  "structure": "Genders are collected in the same file to allow easy sharing of patronym and byname tables. Subtables have been split by their initial letter to allow future support for allitteration and for example siblings sharing the same initial.",
  "note": "",
  "to do": "",
  "see": "",
  "date": "2018-05-05",
  "authors": "Ron Calbick, ELF Vesala.",
  "sources": "Freedictionary.com, name_f_1.json, name_f_2.json, name_m_1.json, name_m_2.json",
