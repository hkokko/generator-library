    {
      "name": "detail",
      "explanation": "More detailed output variations.",
      "entries": [
        "{face} with {feature}",
        "{attribute} {!synonym} and {face} with {feature}"
      ]
    },
    {
      "name": "description",
      "explanation": "Long description as a sentence.",
      "entries": [
        "The {face} has {feature}.",
        "The {attribute} {!synonym} with {face} has {feature}."
      ]
    },
    {
      "name": "section",
      "explanation": "Long description as a subheading and a text paragraph.",
      "entries": [
        "===== {random} =====\r\n{description}"
